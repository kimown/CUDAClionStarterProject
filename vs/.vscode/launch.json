        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "CUDA C++: Launch233",
            "type": "cuda-gdb",
            "request": "launch",
            "cwd": "/home/google/CLionProjects/CUDAClionStarterProject/vs",
            "program": "/usr/bin/python3",
            "args": "-m pycuda.debug /home/google/CLionProjects/CUDAClionStarterProject/vs/a.py",
            "initCommands": [
                "set breakpoint pending on",
                "break /home/google/CLionProjects/CUDAClionStarterProject/vs/cache/tmp/kernel.cu:8",
                "file /usr/bin/python3"
            ]
}
    ]
}
